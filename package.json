    "@babel/core": "^7.11.1",
    "@babel/register": "^7.10.5",
    "regenerator-runtime": "^0.13.7",
    "gulp-sourcemaps": "^2.6.5",
    "del": "^5.1.0",
    "requirejs": "^2.3.6",
    "text": "requirejs/text",
    "gulp-babel": "^8.0.0",
